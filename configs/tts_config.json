{
    "tts_engines": [
        {
            "name": "Tortoise",
            "parameters": [
                {
                    "label": "Autoregressive Model Path",
                    "type": "combobox",
                    "attribute": "autoregressive_model_path",
                    "function": "get_combobox_items",
                    "folder_path": "engines/tortoise/models",
                    "look_for": "files",
                    "file_filter": "*.pth",
                    "include_none_option": true
                },
                {
                    "label": "Diffusion Model Path",
                    "type": "file",
                    "attribute": "diffusion_model_path",
                    "file_filter": "Model Files (*.pth *.pt);;All Files (*)"
                },
                {
                    "label": "Vocoder Name",
                    "type": "text",
                    "attribute": "vocoder_name"
                },
                {
                    "label": "Tokenizer JSON Path",
                    "type": "combobox",
                    "attribute": "tokenizer_json_path",
                    "function": "get_combobox_items",
                    "folder_path": "engines/tortoise/tokenizers",
                    "look_for": "files",
                    "file_filter": "*.json",
                    "include_none_option": true
                },
                {
                    "label": "Voice",
                    "type": "combobox",
                    "attribute": "voice",
                    "function": "get_combobox_items",
                    "folder_path": "./voices",
                    "look_for": "folders",
                    "file_filter": null
                },
                {
                    "label": "Sample Size",
                    "type": "spinbox",
                    "attribute": "sample_size",
                    "min": 1,
                    "max": 64,
                    "default": 4
                },
                {
                    "label": "Use DeepSpeed",
                    "type": "checkbox",
                    "attribute": "use_deepspeed"
                },
                {
                    "label": "Use HiFi-GAN",
                    "type": "checkbox",
                    "attribute": "use_hifigan"
                }
            ]
        },
        {
            "name": "pyttsx3",
            "parameters": [
                {
                    "label": "Voice Rate",
                    "type": "spinbox",
                    "attribute": "rate",
                    "min": 100,
                    "max": 200,
                    "default": 150
                },
                {
                    "label": "Volume",
                    "type": "spinbox",
                    "attribute": "volume",
                    "min": 0,
                    "max": 100,
                    "default": 100
                }
            ]
        },
        {
            "name": "StyleTTS2",
            "parameters": [
                {
                    "label": "Model to Use",
                    "type": "combobox",
                    "attribute": "stts_model_path",
                    "function": "get_combobox_items",
                    "folder_path":"engines/styletts",
                    "look_for":"folders",
                    "file_filter":null,
                    "include_none_option":false
                },
                {
                    "label": "Voice",
                    "type": "combobox",
                    "attribute": "stts_voice",
                    "function": "get_combobox_items",
                    "folder_path":"./voices/styletts",
                    "look_for":"folders",
                    "file_filter":null,
                    "include_none_option":false
                },
                {
                    "label": "Voice Reference File",
                    "type": "combobox",
                    "attribute": "stts_reference_audio_file",
                    "function": "get_combobox_items",
                    "relies_on":"stts_voice",
                    "folder_path":"./voices/styletts",
                    "look_for":"files",
                    "file_filter": "*"
                },
                {
                    "label": "Seed",
                    "type": "text",
                    "attribute": "stts_seed"
                },
                {
                    "label": "Diffusion Steps",
                    "type": "spinbox",
                    "attribute": "stts_diffusion_steps",
                    "min": 1,
                    "max": 1000,
                    "default": 20
                },
                {
                    "label": "Alpha",
                    "attribute": "stts_alpha",
                    "type": "slider",
                    "min": 0,
                    "max": 200,
                    "default": 70,
                    "step":100
                  },
                  {
                    "label": "Beta",
                    "attribute": "stts_beta",
                    "type": "slider",
                    "min": 0,
                    "max": 200,
                    "default": 30,
                    "step":100
                  },
                  {
                    "label": "Embedding Scale",
                    "attribute": "stts_embedding_scale",
                    "type": "slider",
                    "min": 0,
                    "max": 200,
                    "default": 50,
                    "step":100
                  }
            ]
        },
        {
            "name": "XTTS (in progress)",
            "parameters": [
                {
                    "label": "Parameter 1",
                    "type": "text",
                    "attribute": "param1"
                },
                {
                    "label": "Parameter 2",
                    "type": "checkbox",
                    "attribute": "param2"
                }
            ]
        },
        {
            "name": "F5TTS",
            "parameters": [
                {
                    "label":"Voice to Use",
                    "type":"combobox",
                    "attribute":"f5tts_voice",
                    "function":"get_combobox_items",
                    "folder_path":"voices/f5tts",
                    "look_for":"folders",
                    "include_none_option":false
                },
                {
                    "label": "Model to Use",
                    "type": "combobox",
                    "attribute": "f5tts_model",
                    "function" : "get_combobox_items",
                    "folder_path": "engines/f5tts/models",
                    "look_for": "files",
                    "file_filter": "*.pt;*.safetensors",
                    "include_none_option":true
                },
                {
                    "label": "Tokenizer to Use",
                    "type": "combobox",
                    "attribute": "f5tts_tokenizer",
                    "function": "get_combobox_items",
                    "folder_path": "engines/f5tts/tokenizers",
                    "look_for":"files",
                    "file_filter":"*.txt",
                    "include_none_option":true
                },
                {
                    "label": "Vocoder",
                    "type": "combobox",
                    "attribute": "f5tts_vocoder",
                    "function": "get_combobox_items",
                    "folder_path": "engines/f5tts/vocoders",
                    "look_for":"custom",
                    "custom_options": ["vocos", "bigvgan"]
                },
                {
                    "label": "Use Duration Prediction Model?",
                    "type": "checkbox",
                    "attribute": "f5tts_duration_model",
                    "folder_path": "engines/f5tts/duration"
                },
                {
                    "label":"Seed (-1 is random)",
                    "type": "spinbox",
                    "attribute": "f5tts_seed",
                    "min" : -1,
                    "max" : 999999,
                    "default": -1
                }
            ]
        }
    ]
}
