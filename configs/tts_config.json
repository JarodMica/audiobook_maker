                },
                {
                    "label": "VITS Model to Use",
                    "type": "combobox",
                    "attribute": "gpt_sovits_vits_model",
                    "function": "get_combobox_items",
                    "folder_path": "engines/gpt_sovits/sovits_models",
                    "look_for": "files",
