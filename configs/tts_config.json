                },
                {
                    "label": "Speed of Output",
                    "type": "slider",
                    "attribute": "gpt_sovits_speed",
                    "min": 1,
                    "max": 200,
                    "default": 100,
                    "step": 100
                },
                {
                    "label": "Sample Steps",
                    "type": "slider",
                    "attribute": "gpt_sovits_sample_steps",
                    "min": 4,
                    "max": 100,
                    "default": 8,
                    "step": 1
                },
                {
                    "label": "Temperature",
                    "type": "slider",
                    "attribute": "gpt_sovits_temperature",
                    "min": 1,
                    "max": 100,
                    "default": 70,
                    "step": 100
                },
                {
                    "label": "Top k",
                    "type": "slider",
                    "attribute": "gpt_sovits_top_k",
