                {
                    "label": "Use Duration Prediction Model?",
                    "type": "checkbox",
                    "attribute": "f5tts_duration_model",
                    "folder_path": "engines/f5tts/duration"
                },
                {
                    "label": "Speed of Output",
                    "type": "slider",
                    "attribute": "f5tts_speed",
                    "min": 1,
                    "max": 200,
                    "default": 100,
